# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp
/.sass-cache

# Ignore configuration files
/.env*
!/.env.example

# Uploaded files during development
/public/system
/public/uploads

/spec/factories
/.

# API feed samples for working locally
/feeds

/.powrc

.DS_Store
dump.rdb

/.vagrant
/rails_app
